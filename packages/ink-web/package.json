{
  "name": "ink-web",
  "version": "0.1.15",
  "description": "Run Ink (React for CLIs) in the browser using Xterm.js",
  "type": "module",
  "workspaces": [
    "docs"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup && bun run scripts/fix-bundled-fs.ts",
    "build:watch": "tsup --watch",
    "dev": "bun run build && bunx vite --config vite.config.ts",
    "dev:watch": "bunx concurrently \"bun run build:watch\" \"bunx vite --config vite.config.ts\"",
    "test": "bun test --preload ./test/preload.ts",
    "prepublishOnly": "bun run build",
    "docs": "cd docs && bun run dev",
    "docs:build": "bun run build && cd docs && bun run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./css": {
      "types": "./dist/index.css.d.ts",
      "import": "./dist/index.css"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.js",
      "require": "./dist/vite-plugin.cjs"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "peerDependencies": {
    "ink": "^6.0.0",
    "react": "^19.0.0",
    "react-reconciler": "^0.33.0",
    "scheduler": "^0.27.0",
    "vite": ">=7.0.0",
    "xterm": ">=5.0.0"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.10.0"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^20.0.11",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.3.3",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@vitejs/plugin-react": "^5.1.1",
    "chalk": "^5.6.2",
    "ink": "^6.5.1",
    "next": "^16.0.7",
    "path-browserify": "^1.0.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "scheduler": "^0.27.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "xterm": "^5.3.0"
  },
  "keywords": [
    "ink",
    "xterm",
    "terminal",
    "browser",
    "react",
    "cli",
    "tui"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/ink-web#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ink-web.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ink-web/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
